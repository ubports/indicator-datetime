msgstr "%s (έχει αφυπνίσεις)"

#. * strftime(3) format for abbreviated weekday,
#. hours, minutes in a 12h locale; e.g. Wed, 2:00 PM
#: ../src/snap.cpp:144
msgid "%a, %l:%M %p"
msgstr "%a, %l:%M %p"

#. * A strftime(3) format for abbreviated weekday,
#. hours, minutes in a 24h locale; e.g. Wed, 14:00
#: ../src/snap.cpp:148
msgid "%a, %H:%M"
msgstr "%a, %H:%M"

