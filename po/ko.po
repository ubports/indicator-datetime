#: ../src/menu.cpp:596
msgid "Time and Date"
msgstr "시간 및 날짜"

#: ../src/menu.cpp:601
#, c-format
msgid "%s (has alarms)"
msgstr "%s (알람 있음)"

#. * strftime(3) format for abbreviated weekday,
#. hours, minutes in a 12h locale; e.g. Wed, 2:00 PM
#: ../src/snap.cpp:144
msgid "%a, %l:%M %p"
msgstr "%p %l:%M (%a)"

#. * A strftime(3) format for abbreviated weekday,
#. hours, minutes in a 24h locale; e.g. Wed, 14:00
#: ../src/snap.cpp:148
msgid "%a, %H:%M"
msgstr "%H:%M (%a)"

